<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:backgroundTint="#ffffff"
    tools:context=".ResultActivity"><![CDATA[
    app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.919"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.497" />










    ]]>

    <LinearLayout
        android:id="@+id/place_details_layout"
        android:layout_width="414dp"
        android:layout_height="89dp"
        android:layout_marginTop="32dp"
        android:background="#ffffff"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="-2dp">

        <TextView
            android:id="@+id/place_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:gravity="center"
            android:textSize="25sp"
            tools:layout_conversion_absoluteHeight="34dp"
            tools:layout_conversion_absoluteWidth="411dp" />

        <TextView
            android:id="@+id/place_address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="          "
            android:textSize="14sp"
            tools:layout_conversion_absoluteHeight="19dp"
            tools:layout_conversion_absoluteWidth="411dp" />
    </LinearLayout>

    <Button
        android:id="@+id/add_review_intent"
        style="drawable/button"
        android:layout_width="173dp"
        android:layout_height="69dp"
        android:layout_marginStart="28dp"
        android:background="@drawable/button"
        android:fadeScrollbars="true"
        android:text="@string/add_review_button"
        android:textIsSelectable="false"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@id/open_map_intent"
        app:layout_constraintTop_toBottomOf="@id/place_details_layout" />

    <Button
        android:id="@+id/open_map_intent"
        style="drawable/button"
        android:layout_width="173dp"
        android:layout_height="69dp"
        android:layout_marginEnd="212dp"
        android:background="@drawable/button"
        android:fadeScrollbars="true"
        android:text="@string/open_map_button"
        android:textIsSelectable="false"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/place_details_layout" />

    <TextView
        android:id="@+id/avg_grade"
        android:layout_width="44dp"
        android:layout_height="33dp"
        android:text="      "
        android:textColor="#730E9E"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@id/legend_frame"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/avg_grade_text"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/avg_grade_text"
        android:layout_width="407dp"
        android:layout_height="39dp"
        android:layout_marginBottom="31dp"
        android:fadeScrollbars="true"
        android:gravity="center"
        android:text="@string/avg_grade"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@id/avg_grade"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/open_map_intent"
        app:layout_constraintVertical_bias="0.13" />

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="410dp"

        android:layout_height="323dp"
        android:background="@color/common_google_signin_btn_text_light_disabled"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/legend_frame"
        app:layout_constraintVertical_bias="1.0"
        tools:context="com.example.test.listviewwithimage.MainActivity">

        <TextView
            android:id="@+id/no_reviews_yet"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="7dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="372dp"
            android:text="@string/no_reviews_yet"
            android:textColor="#730E9E"
            android:textSize="24sp"
            android:visibility="invisible" />

        <ListView
            android:id="@+id/list"
            style="@android:style/Widget.DeviceDefault.Light.ExpandableListView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="1dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="-1dp"
            android:layout_marginBottom="14dp"
            android:background="#ffffff"
            android:backgroundTint="#FFFFFF"
            android:divider="#000000"
            android:dividerHeight="1dp">

        </ListView>

    </RelativeLayout>

    <FrameLayout
        android:id="@+id/legend_frame"
        android:layout_width="382dp"
        android:layout_height="61dp"
        app:layout_constraintBottom_toTopOf="@+id/relativeLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/avg_grade"
        app:layout_constraintVertical_bias="0.903">


        <LinearLayout
            android:id="@+id/legend_layout"
            android:layout_width="wrap_content"
            android:layout_height="58dp"
            android:divider="#A1252525"
            android:dividerPadding="1dp"
            android:orientation="horizontal"
            android:showDividers="middle">

            <LinearLayout
                android:layout_width="102dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/isVX4"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/v4" />

                <TextView
                    android:id="@+id/service_final"
                    android:layout_width="71dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:paddingStart="1dp"
                    android:text="@string/accessible_crew"
                    android:textSize="14sp"
                    android:textStyle="bold" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="102dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/isVX1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/v1" />

                <TextView
                    android:id="@+id/parking_final"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="0"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="@string/accessible_parking"
                    android:textSize="14sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="102dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/isVX2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/v2" />

                <TextView
                    android:id="@+id/accessibility_final"
                    android:layout_width="69dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:paddingStart="1dp"
                    android:text="@string/accessible_arriving"
                    android:textSize="14sp"
                    android:textStyle="bold" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="102dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">


                <ImageView
                    android:id="@+id/isVX3"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/v3" />

                <TextView
                    android:id="@+id/toilet_final"
                    android:layout_width="wrap_content"
                    android:layout_height="37dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingStart="1dp"
                    android:text="@string/accessible_toilet"
                    android:textSize="14sp"
                    android:textStyle="bold" />

            </LinearLayout>

        </LinearLayout>

    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>