<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultActivity"><![CDATA[
    app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.919"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.497" />
    ]]>

    <ImageView
        android:id="@+id/main_background_light"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/main_background_light" />

    <TextView
        android:id="@+id/place_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:text="          "
        android:textColor="#202647"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.474"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <TextView
        android:id="@+id/place_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:text="          "
        android:textColor="#293468"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/place_name"
        app:layout_constraintVertical_bias="0.013" />

    <Button
        android:id="@+id/add_review_intent"
        style="@style/Base.AlertDialog.AppCompat.Light"
        android:layout_width="165dp"
        android:layout_height="60dp"
        android:background="@drawable/button"
        android:backgroundTint="#6A90A3C4"
        android:fadeScrollbars="true"
        android:text="@string/add_review_button"
        android:textColor="#D73A3A3A"
        android:textIsSelectable="false"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toEndOf="@+id/open_map_intent"
        app:layout_constraintTop_toBottomOf="@+id/place_address"
        app:layout_constraintVertical_bias="0.015" />

    <Button
        android:id="@+id/open_map_intent"
        style="@style/Base.AlertDialog.AppCompat.Light"
        android:layout_width="165dp"
        android:layout_height="60dp"
        android:background="@drawable/button"
        android:backgroundTint="#6A90A3C4"
        android:fadeScrollbars="true"
        android:text="@string/open_map_button"
        android:textColor="#D73A3A3A"
        android:textIsSelectable="false"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/avg_grade_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.142"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/place_address"
        app:layout_constraintVertical_bias="0.333" />

    <TextView
        android:id="@+id/avg_grade"
        android:layout_width="44dp"
        android:layout_height="33dp"
        android:text="      "
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/relativeLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/avg_grade_text"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/avg_grade_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="31dp"
        android:backgroundTint="#6A90A3C4"
        android:fadeScrollbars="true"
        android:text="@string/avg_grade"
        android:textColor="#D73A3A3A"
        android:textSize="18sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/relativeLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/add_review_intent"
        app:layout_constraintVertical_bias="0.13" />

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="404dp"

        android:layout_height="442dp"
        android:background="@color/common_google_signin_btn_text_light_disabled"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/add_review_intent"
        app:layout_constraintVertical_bias="1.0"
        tools:context="com.example.test.listviewwithimage.MainActivity">

        <TextView
            android:id="@+id/no_reviews_yet"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="7dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="372dp"
            android:text="@string/no_reviews_yet"
            android:textSize="24sp"
            android:visibility="invisible" />

        <ListView
            android:id="@+id/list"
            style="@android:style/Widget.DeviceDefault.Light.ExpandableListView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="1dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="-1dp"
            android:layout_marginBottom="14dp"
            android:divider="@color/quantum_black_text"
            android:dividerHeight="@dimen/places_autocomplete_vertical_dropdown">

        </ListView>

    </RelativeLayout>

    <FrameLayout
        android:id="@+id/legend_frame"
        android:layout_width="382dp"
        android:layout_height="61dp"
        app:layout_constraintBottom_toTopOf="@+id/relativeLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/avg_grade"
        app:layout_constraintVertical_bias="0.0">


        <LinearLayout
            android:id="@+id/legend_layout"
            android:layout_width="wrap_content"
            android:layout_height="58dp"
            android:divider="@drawable/places_autocomplete_toolbar_shadow"
            android:dividerPadding="1dp"
            android:orientation="horizontal"
            android:showDividers="middle">

            <LinearLayout
                android:layout_width="102dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/isVX4"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/v4" />

                <TextView
                    android:id="@+id/service_final"
                    android:layout_width="71dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:paddingStart="1dp"
                    android:text="@string/accessible_crew"
                    android:textSize="14sp"
                    android:textStyle="bold" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="102dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/isVX1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/v1" />

                <TextView
                    android:id="@+id/parking_final"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="0"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="@string/accessible_parking"
                    android:textSize="14sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="102dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/isVX2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/v2" />

                <TextView
                    android:id="@+id/accessibility_final"
                    android:layout_width="69dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:paddingStart="1dp"
                    android:text="@string/accessible_arriving"
                    android:textSize="14sp"
                    android:textStyle="bold" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="102dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">


                <ImageView
                    android:id="@+id/isVX3"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/v3" />

                <TextView
                    android:id="@+id/toilet_final"
                    android:layout_width="wrap_content"
                    android:layout_height="37dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingStart="1dp"
                    android:text="@string/accessible_toilet"
                    android:textSize="14sp"
                    android:textStyle="bold" />

            </LinearLayout>

        </LinearLayout>

    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>